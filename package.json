{
  "name": "rcauquil",
  "description": "Personnal website using sapper/svelte and mdsvex",
  "version": "1.1.0",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/styles/index.css -w",
    "build:tailwind": "postcss static/tailwind.css -o static/styles/index.css",
    "dev": "sapper dev --ext '.svelte .svx'",
    "start": "run-p watch:tailwind dev",
    "export": "NODE_ENV=production npm run build:tailwind && sapper export --ext '.svelte .svx'"
  },
  "dependencies": {
    "compression": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.0",
    "config": "^3.3.1",
    "cssnano": "^4.1.10",
    "front-matter": "^4.0.2",
    "fs": "0.0.1-security",
    "lodash": "^4.17.19",
    "mdsvex": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "polka": "^0.5.2",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-purgecss": "^2.0.3",
    "prismjs": "^1.20.0",
    "rollup": "^2.15.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.15",
    "sirv": "^1.0.0",
    "svelte": "^3.23.2",
    "svelte-preprocess-postcss": "^1.1.1",
    "tailwindcss": "^1.4.6"
