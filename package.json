{
  "name": "rcauquil",
  "description": "Personnal website using sapper/svelte and mdsvex",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build",
    "dev": "sapper dev --ext '.svelte .svx'",
    "start": "run-p watch:tailwind dev",
    "export": "sapper export --ext '.svelte .svx'"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
